{
  "name": "@iml/deep-freeze",
  "version": "2.0.2",
  "description": "Recursively freezes a JSON like object tree",
  "main": "source/index.js",
  "scripts": {
    "test": "jest",
    "cover": "NODE_ENV=test npm t -- --coverage",
    "flow": "flow",
    "eslint": "eslint ./",
    "postversion": "rm -rf ./dist && babel source --out-dir dist --copy-files"
