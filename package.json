{
  "name": "@mfl/deep-freeze",
  "version": "2.0.2",
  "description": "Recursively freezes a JSON like object tree",
  "main": "source/index.js",
  "scripts": {
    "test": "jest",
    "cover": "NODE_ENV=test npm t -- --coverage",
    "flow": "flow",
    "eslint": "eslint ./",
    "postversion": "rm -rf ./dist && babel source --out-dir dist --copy-files"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/deep-freeze.git"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-strict-mode": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.10.2",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "0.35.0",
    "@mfl/jasmine-n-matchers": "2.1.1",
    "jasmine-reporters": "2.2.0",
    "jest-cli": "17.0.3",
    "pre-commit": "1.1.3"
  },
  "dependencies": {}
}
